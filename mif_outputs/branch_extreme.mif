-- null
DEPTH = 4096;
WIDTH = 32;

ADDRESS_RADIX = DEC;
DATA_RADIX = BIN;

CONTENT
BEGIN
    -- addi $1, $0, -1      # $1 = -1
0000 : 00101000010000011111111111111111;
    -- addi $2, $0, 0       # $2 = 0
0001 : 00101000100000000000000000000000;
    -- blt  $1, $2, L1      # -1 < 0 -> should take
0002 : 00110000010001000000000000000001;
    -- addi $10, $0, 1      # skipped if branch taken
0003 : 00101010100000000000000000000001;
    -- L1: nop
0004 : 00000000000000000000000000000000;
    -- addi $10, $0, 5      # $10 = 5
0005 : 00101010100000000000000000000101;
[0006 .. 4095] : 00000000000000000000000000000000;
END;
