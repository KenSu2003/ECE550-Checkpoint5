-- null
DEPTH = 4096;
WIDTH = 32;

ADDRESS_RADIX = DEC;
DATA_RADIX = BIN;

CONTENT
BEGIN
    -- addi $4, $0, 5       # $4 = 5      (target PC for jr)
0000 : 00101001000000000000000000000101;
    -- addi $1, $0, 1       # $1 = 1
0001 : 00101000010000000000000000000001;
    -- jr   $4              # PC = $4 -> next executed instruction is index 5
0002 : 00100001000000000000000000000000;
    -- addi $1, $0, 99      # skipped
0003 : 00101000010000000000000001100011;
    -- addi $1, $0, 99      # skipped
0004 : 00101000010000000000000001100011;
    -- addi $2, $0, 2       # label target: $2 = 2
0005 : 00000000000000000000000000000000;
[0006 .. 4095] : 00000000000000000000000000000000;
END;
