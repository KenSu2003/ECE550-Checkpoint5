-- null
DEPTH = 4096;
WIDTH = 32;

ADDRESS_RADIX = DEC;
DATA_RADIX = BIN;

CONTENT
BEGIN
    -- addi $1, $0, 7        # $1 = 7
0000 : 00101000010000000000000000000111;
    -- addi $2, $0, 8        # $2 = 8
0001 : 00101000100000000000000000001000;
    -- jal FUNC              # $31 = return addr (PC+1), jump to FUNC
0002 : 00011000000000000000000000000101;
    -- addi $5, $0, 9        # executed after returning (marker)
0003 : 00101001010000000000000000001001;
    -- nop
0004 : 00000000000000000000000000000000;
    -- FUNC: nop
0005 : 00000000000000000000000000000000;
    -- addi $3, $0, -5       # $3 = -5
0006 : 00101000110000011111111111111011;
    -- blt  $3, $1, Lblt     # -5 < 7 -> branch taken to Lblt
0007 : 00110000110000100000000000000001;
    -- addi $28, $0, 99      # skipped if branch taken
0008 : 00101111000000000000000001100011;
    -- Lblt: nop
0009 : 00000000000000000000000000000000;
    -- addi $28, $0, 4       # $28 = 4  (marker)
0010 : 00101111000000000000000000000100;
    -- jr   $31              # return (PC <- $31)
0011 : 00100111110000000000000000000000;
[0012 .. 4095] : 00000000000000000000000000000000;
END;
