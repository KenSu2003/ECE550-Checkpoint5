-- null
DEPTH = 4096;
WIDTH = 32;

ADDRESS_RADIX = DEC;
DATA_RADIX = BIN;

CONTENT
BEGIN
    -- addi $1, $0, 1        # $1 = 1           (instr 0)
0000 : 00101000010000000000000000000001;
    -- jal  L1               # $31 = PC+1 (=2), PC -> L1 (instr 2)
0001 : 00011000000000000000000000000011;
    -- addi $1, $0, 99       # skipped (instr 2)
0002 : 00101000010000000000000001100011;
    -- L1: nop
0003 : 00000000000000000000000000000000;
    -- addi $2, $0, 2        # $2 = 2           (instr 3)
0004 : 00101000100000000000000000000010;
    -- j    L2               # jump to L2 (instr 5)
0005 : 00001000000000000000000000000111;
    -- addi $2, $0, 99       # skipped (instr 5)
0006 : 00101000100000000000000001100011;
    -- L2: nop
0007 : 00000000000000000000000000000000;
    -- addi $3, $0, 3        # $3 = 3
0008 : 00101000110000000000000000000011;
[0009 .. 4095] : 00000000000000000000000000000000;
END;
